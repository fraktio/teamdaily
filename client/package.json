{
  "name": "teamdaily-client",
  "version": "3.0.0",
  "description": "TeamDaily client",
  "main": "src",
  "lint-staged": {
    "gitDir": "..",
    "linters": {
      "*.js": [
        "./node_modules/.bin/prettier --single-quote --print-width 100 --trailing-comma all --write",
        "git add"
      ],
      "*.json": ["./node_modules/.bin/prettier --parser json --print-width 100 --write", "git add"],
      "*.css": [
        "./node_modules/.bin/prettier --parser postcss --print-width 100 --write",
        "git add"
      ],
      "*.pcss": [
        "./node_modules/.bin/prettier --parser postcss --print-width 100 --write",
        "git add"
      ]
    }
  },
  "scripts": {
    "precommit": "lint-staged",
    "clean": "rimraf ./dist",
    "build:client": "cross-env NODE_ENV=production webpack --progress",
    "build": "yarn run clean && yarn run build:client",
    "lint": "eslint src --ext js",
    "babel-node": "babel-node",
    "start": "cross-env NODE_ENV=development ./node_modules/.bin/nodemon --exec yarn run babel-node -w src/dev.js -- src/dev.js 8765",
    "test": "jest",
    "flow": "flow"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fraktio/teamdaily-os"
  },
  "keywords": ["TeamDaily"],
  "author": "Fraktio",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.23.0",
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "moment": "^2.18.1",
    "react": "^15.6.1",
    "react-addons-perf": "^15.4.1",
    "react-apollo": "^1.4.8",
    "react-dom": "^15.6.1",
    "react-fa": "^4.2.0",
    "react-immutable-proptypes": "^2.1.0",
    "react-intl": "^2.3.0",
    "react-keydown": "^1.7.7",
    "react-masonry-component": "^5.0.7",
    "react-modal": "^2.2.2",
    "react-modal-dialog": "^4.0.7",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "recompose": "^0.24.0",
    "redbox-react": "^1.5.0",
    "redux": "^3.7.2",
    "redux-promise-middleware": "4.2.1",
    "redux-thunk": "^2.1.0",
    "screenfull": "^3.3.1",
    "styled-components": "^2.1.1",
    "uuid": "^3.1.0"
  },

{
  "name": "teamdaily-server",
  "version": "3.0.0",
  "description": "TeamDaily Server",
  "main": "src",
  "lint-staged": {
    "gitDir": "..",
    "linters": {
      "*.js": [
        "./node_modules/.bin/prettier --single-quote --print-width 100 --trailing-comma all --write",
        "git add"
      ],
      "*.json": [
        "./node_modules/.bin/prettier --parser json --print-width 100 --write",
        "git add"
      ]
    }
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "cross-env babel-watch -L src/api.js",
    "babel-watch": "babel-watch",
    "test": "jest",
    "db-migrate": "db-migrate"
  },
  "engines": {
    "node": ">= 8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fraktio/teamdaily-os"
  },
  "keywords": ["TeamDaily"],
  "author": "Fraktio",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-watch": "^2.0.7",
    "body-parser": "^1.17.2",
    "cors": "^2.8.3",
    "cross-env": "^5.0.1",
    "db-migrate": "^0.10.0-beta.21",
    "db-migrate-mysql": "^1.1.10",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "firebase-admin": "^5.4.3",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "mysql": "^2.13.0",
    "promise-mysql": "^3.0.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "faker": "^4.1.0",
    "immutable": "^3.8.1",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.2",
    "nodemon": "^1.11.0",
    "prettier": "^1.5.3",
    "random-js": "^1.0.8"
  }
}
